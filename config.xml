<?xml version='1.0' encoding='utf-8'?>
<widget id="com.terzian.app" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>TerzianApp</name>
    <description>Cordova-AngularJS-angularMaterial Seed.</description>
    <author email="andresmartindoria@gmail.com" href="https://github.com/mario-aleo">Andres Doria</author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <access origin="*" />
    <icon src="www/assets/img/ico-app.png" />
    <access origin="http://*.google.com" />
    <access origin="http://maps.google.com" />
    <access origin="https://google.com" />
    <access origin="http://google.com" />
    <access origin="*.google.com" />
    <access origin="*.googleapis.com" />
    <access origin="*.gstatic.com" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <uses-permission name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <plugin name="cordova-plugin-statusbar" spec="~2.1.2" />
    <plugin name="cordova-plugin-splashscreen" spec="~3.2.1" />
    <plugin name="cordova-plugin-geolocation" spec="^4.0.1" />
    <plugin name="cordova-plugin-file-transfer" spec="^1.7.1" />
    <engine name="ios" spec="~4.1.0" />
    <engine name="windows" spec="~4.3.1" />
    <engine name="browser" spec="^5.0.4" />
    <engine name="android" spec="^7.1.4" />
</widget>
